        "tsconfig-paths-webpack-plugin": "4.0.0",
        "typescript": "4.9.5"
    },
    "resolutions": {
        "webpack": "^5"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,}": [
            "eslint --max-warnings=0 --fix",
            "prettier -w"
        ],
        "**/*.{json,css,scss,md}": [
            "prettier -w"
        ],
        "types/**/*.d.ts": [
            "prettier -w"
        ]

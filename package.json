        "@vgrid/react-leaflet-heatmap-layer": "4.0.3",
        "axios": "1.3.4",
        "clsx": "1.2.1",
        "leaflet": "1.9.3",
        "multiparty": "4.2.3",
        "next": "13.3.0",
        "next-connect": "1.0.0-next.3",
        "next-superjson": "0.0.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
